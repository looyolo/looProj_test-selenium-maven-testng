<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- Default Suite -->
<suite name="Default Suite" parallel="tests" thread-count="2">
    <!-- Java_Learn -->
    <test name="FirefoxTest">
        <!-- 添加参数值对，可以是多个 -->
        <!--  浏览器设置  -->
